{
  "name": "cooking_book",
  "version": "1.0.0",
  "description": "a modern webapp for your recipes using React, Node.js, MongoDB and Webpack.",
  "engines": {
    "node": ">= 8",
    "npm": ">= 5"
  },
  "main": "src/server/index.js",
  "scripts": {
    "start": "node src/server/index.js",
    "build": "webpack --mode production --progress",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "nodemon src/server/index.js",
    "dev": "cross-env NODE_ENV=dev webpack-dev-server --progress --hot --open",
    "dev:debug": "nodemon --inspect src/server/index.js",
    "dev:client": "webpack --progress --watch"
  },
  "keywords": [
    "food",
    "recipes",
    "website",
    "webapp"
  ],
  "author": "Funnybobby",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/funnybobby2/repository.git"
  },
  "bugs": {
